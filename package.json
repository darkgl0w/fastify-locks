{
  "name": "fastify-locks",
  "version": "2.0.0",
  "description": "A Fastify Web Locks API Plugin",
  "main": "./plugin.js",
  "exports": {
    "import": "./esm-wrapper.mjs",
    "require": "./plugin.js"
  },
  "types": "./plugin.d.ts",
  "scripts": {
    "build": "gen-esm-wrapper . esm-wrapper.mjs",
    "lint": "standardx \"**/*.{mjs,js,cjs}\" | snazzy",
    "prepack": "npm run build",
    "test:typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piscinajs/fastify-locks"
  },
  "keywords": [
    "piscina",
    "weblocks",
    "fastify"
  ],
  "author": "James M Snell <jasnell@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "fastify": "^3.24.1",
    "gen-esm-wrapper": "^1.1.3",
    "snazzy": "^9.0.0",
    "standardx": "^7.0.0",
    "tap": "^15.1.5",
    "tsd": "^0.19.0"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "piscina-locks": "^2.0.2"
  },
  "eslintConfig": {
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": "off"
    },
    "globals": {
      "SharedArrayBuffer": true,
      "Atomics": true
    }
  },
  "standardx": {},
  "tsd": {
    "directory": "test/types"
  }
}
